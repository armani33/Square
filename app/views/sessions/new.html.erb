  <% provide(:title, "Log In") %>

  <!-- Page Content -->
  <div class="container text-center">
    <div class="row">
      <%= image_tag('Logo.png', class: "sign-up", width: "100", height: "100") %>
      <h3 class=sign-up>Log In</h3>
      <hr>
    </div>
    <%= form_for(:session, url: login_path) do |f| %>
      <div class="row">
        <div class="col-lg-4 col-lg-offset-4">
          <%= f.label :email %><br>
          <%= f.email_field :email, class: "form-control" %><br>
        </div>
      </div>

      <div class="row">
        <div class="col-lg-4 col-lg-offset-4">
          <%= f.label :password %><br>
          <%= f.password_field :password, class: "form-control" %><br>
        </div>
      </div>

      <%= f.submit "Login", class: "btn btn-singup" %><br><br>
    <% end %>

    <p class=sign-up>Don't have an account? <%= link_to "Sign Up Here", signup_path %></p>
  </div>
